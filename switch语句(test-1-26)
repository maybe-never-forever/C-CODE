int main()
{
	int ch = 0;
	//ctrl+z
	//EOF -end of file-> -1
	while ((ch= getchar()) != EOF)
	{
		putchar(ch);
	}
	//putchar(ch);
	//printf("%c\n", ch);
	return 0;
}
